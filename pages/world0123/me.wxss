/* pages/home/home.wxss */
.page{  
  height: 100%;
  width: 100%;
  background-color: #f8f8f9;
}  

.card_container{
  height: 93%;
  color: #008080;
}

/* 主页 */  
.home {  
  position: absolute;  
  width: 750rpx;  
  height: 100%;  
  z-index: 1;  
  transition: All 0.4s ease;  
  -webkit-transition: All 0.4s ease;  
  background-color: rgb(255,255,255);
}  
.icon_user{
  height: 50px;
  width: 45px;
  position: relative;
  top: 5px;
  left: 3px;
}
.top{
  background-color: rgb(255, 255, 255);
  height: 40px;
}
.txt{
  position: relative;
  left: 160px;
  top: 5px;  
  color:rgb(20, 20, 20);
}


/* 遮罩层 */  
.bg {    
  display: none;    
  position: fixed;    
  top: 0%;    
  left: 70%;    
  width: 100%;    
  height: 100%;    
  background-color: rgb(31, 30, 30);    
  z-index: 1001;    
  -moz-opacity: 0.7;    
  opacity: 0.70;    
  transition:width 2s;  
  filter: alpha(opacity=70);    
}   

 /* 侧滑栏 */  
.page_tab {  
  height: 100%;  
  width: 750rpx;  
  position: fixed;  
  z-index: 0; 
}  
.bgbody{
  opacity:0.8;
  height: 600px;
}
  
.page_tab_content .wc {  
  color: #000;  
  position: relative;  
  font-size: 32rpx;  
  padding: 20rpx 0 30rpx 20rpx; 
}  
.userinfo {
  display: flex;
  flex-direction: column;
  height: 150px;
  position: relative;
}

.userinfo-avatar {
  width: 140rpx;
  height: 140rpx;
  margin: 20rpx;
  border-radius: 50%;
  position: absolute;
  left: 20px;top: 40px;
  overflow:hidden;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}
.userinfo-nickname {
  color: rgb(247, 208, 34);
  position:absolute;
  left: 130px;top: 84px;
}
.plus-button{
  width: 125rpx;
  height: 125rpx;
  position: absolute;
  bottom:10rpx;
  right: 37.5rpx;
}
.order_item{
  border-radius:35rpx; 
  height: 300rpx;
  position: relative;
  margin: 24rpx 37.5rpx;
  box-shadow: -5rpx 5rpx 10rpx #d6d6d6;
  background-image: url('data:image/jpeg;base64,');
  background-size: cover;
}

.order_item view{
  width: 70%;
  margin: 15rpx 15% ;
  text-align: center;
  height: 100rpx;
}
.emmmm{
  font-weight: bold;
  margin-top: 20rpx;
  font-size: 50rpx;
}
/*去掉滚动条*/
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.wc{  
  color: #000;  
  height: 30px;
  margin-top: 0px;
}  
.pagetxt{
  position: absolute;
  height: 20px;
  font-size: 32rpx; 
}
.pageimg{
  display: inline-block;  
  height: 60rpx;  
  width: 60rpx;  
  margin-right: 60rpx; 
  margin-left: 80rpx;
}


/*图标引入*/

.iconfont {
  font-family: "iconfont" !important;
  font-size: 25px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  position: absolute;
  left: 40px;
  top: 0px;
  color: rgb(149, 226, 6);
}

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1552286571856'); /* IE9 */
  src: url('iconfont.eot?t=1552286571856#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('iconfont.woff?t=1552286571856') format('woff'),
  url('iconfont.ttf?t=1552286571856') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('iconfont.svg?t=1552286571856#iconfont') format('svg'); /* iOS 4.1- */
}


.iconxinzeng:before {
  content: "\e610";
}

.iconyejianmoshi:before {

/* pages/home/home.wxss */
.page{  
  height: 100% ;
}  
/* 主页 */  
.home {  
  position: absolute;  
  width: 750rpx;  
  height: 100%;  
  z-index: 1;  
  transition: All 0.4s ease;  
  -webkit-transition: All 0.4s ease;  
  background-color: rgb(255,255,255);
}  
.icon_user{
  height: 50px;
  width: 45px;
  position: relative;
  top: 5px;
  left: 3px;
}
.top{
  background-color: rgb(255,255,255);
  height: 6%;
}
.txt{
  position: relative;
  left: 160px;
  top: 5px;  
  color:rgb(20, 20, 20);
}


/* 遮罩层 */  
.bg {    
  display: none;    
  position: fixed;    
  top: 0%;    
  left: 70%;    
  width: 100%;    
  height: 100%;    
  background-color: rgb(31, 30, 30);    
  z-index: 1001;    
  -moz-opacity: 0.7;    
  opacity: 0.70;    
  transition:width 2s;  
  filter: alpha(opacity=70);    
}   

 /* 侧滑栏 */  
.page_tab {  
  height: 100%;  
  width: 750rpx;  
  position: fixed;  
  z-index: 0; 
}  
.bgbody{
  opacity:0.8;
  height: 600px;
}
  
.page_tab_content .wc {  
  color: #000;  
  position: relative;  
  font-size: 32rpx;  
  padding: 20rpx 0 30rpx 20rpx; 
}  
.userinfo {
  display: flex;
  flex-direction: column;
  height: 150px;
  position: relative;
}

.userinfo-avatar {
  width: 140rpx;
  height: 140rpx;
  margin: 20rpx;
  border-radius: 50%;
  position: absolute;
  left: 20px;top: 40px;
}
.userinfo-nickname {
  color: rgb(132, 132, 132);
  position:absolute;
  left: 130px;top: 84px;
}
.plus-button{
  width: 125rpx;
  height: 125rpx;
  position: absolute;
  bottom:10rpx;
  right: 37.5rpx;
  box-shadow: 10rpx 10rpx 10rpx #c5c5c5;
  border-radius:100%; 
}
.order_item{
  border-radius:35rpx; 
  height: 300rpx;
  position: relative;
  margin: 20rpx 37.5rpx;
  box-shadow: 10rpx 10rpx 10rpx #c5c5c5;
}

.order_item view{
  width: 70%;
  height: 100rpx;
  margin: 0 15% ;
  text-align: center;
  vertical-align: middle;
}
/*去掉滚动条*/
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.wc{  
  color: #000;  
  height: 30px;
  margin-top: 0px;
}  
.pagetxt{
  position: absolute;
  height: 20px;
  font-size: 32rpx; 
}
.pageimg{
  display: inline-block;  
  height: 60rpx;  
  width: 60rpx;  
  margin-right: 60rpx; 
  margin-left: 80rpx;
}

/*图标引入*/

.iconfont {
  font-family: "iconfont" !important;
  font-size: 25px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  position: absolute;
  left: 40px;
  top: 0px;
}


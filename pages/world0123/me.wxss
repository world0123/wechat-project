.date{
  font-size: 30rpx;
  padding-top: 30rpx;
}
.title{
  font-style:italic;
  font-size: 35rpx;
}
.TXT{
  color:rgb(250, 175, 37);
  font-size:18px;
  padding-left:20px;
}
.btn{
  height:70px;
  width:280px;
  background-color:#ffff;
  opacity:0.9;
